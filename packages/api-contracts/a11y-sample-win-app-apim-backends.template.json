{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ApimServiceName": {
            "type": "string"
        }
    },
    "resources": [
        {
            "properties": {
                "description": "a11y-sample-win-app",
                "resourceId": "https://management.azure.com/subscriptions/76eb140e-abd4-4a17-ad98-534d0f8d2759/resourceGroups/askalbho-test/providers/Microsoft.Web/sites/a11y-sample-win-app",
                "credentials": {
                    "header": {
                        "x-functions-key": ["{{a11y-sample-win-app-key}}"]
                    }
                },
                "url": "https://a11y-sample-win-app.azurewebsites.net/api",
                "protocol": "http"
            },
            "name": "[concat(parameters('ApimServiceName'), '/a11y-sample-win-app')]",
            "type": "Microsoft.ApiManagement/service/backends",
            "apiVersion": "2019-01-01"
        },
        {
            "properties": {
                "description": "demurugefunc",
                "resourceId": "https://management.azure.com/subscriptions/76eb140e-abd4-4a17-ad98-534d0f8d2759/resourceGroups/demurugefunc/providers/Microsoft.Web/sites/demurugefunc",
                "credentials": {
                    "header": {
                        "x-functions-key": ["{{demurugefunc-key}}"]
                    }
                },
                "url": "https://demurugefunc.azurewebsites.net/api",
                "protocol": "http"
            },
            "name": "[concat(parameters('ApimServiceName'), '/demurugefunc')]",
            "type": "Microsoft.ApiManagement/service/backends",
            "apiVersion": "2019-01-01"
        }
    ]
}
