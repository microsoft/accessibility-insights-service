    "@types/leveldown": "^4.0.2",
    "@types/levelup": "^5.1.1",
    "@types/node": "^16.18.11",
    "@types/puppeteer": "^7.0.4",
    "jest": "^29.5.0",
    "jest-junit": "^15.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.2",
    "rollup": "^3.13.0",
    "ts-jest": "^29.1.0",
    "typemoq": "^2.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@axe-core/puppeteer": "^4.5.0",
