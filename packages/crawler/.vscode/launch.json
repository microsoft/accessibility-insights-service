{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Node js debugging",
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}/dist/crawl-url/crawl-url.js",
                "--baseUrl=\"https://education.minecraft.net/wp-login.php?wpe-login=minecraftedu&action=wordpress_social_authenticate&mode=login&provider=Live\""
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9228
        },
        {
            "name": "Debug current unit test file",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}/node_modules/jest/bin/jest.js",
                "--runInBand",
                "${relativeFile}",
                "--coverage",
                "false"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229
        }
    ]
}
