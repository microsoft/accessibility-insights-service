        "create-drop-dir": "npx mkdirp drop",
        "clean": "rimraf dist drop test-results ai_scan_cli_output",
        "lint": "eslint -c ../../.eslintrc.js --ext .ts ./",
        "lint:fix": "eslint --fix -c ../../.eslintrc.js --ext .ts ./",
        "test": "jest --coverage --colors"
    },
