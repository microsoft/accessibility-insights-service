{
    "id": "report-generator-schedule",
    "schedule": {
        "recurrenceInterval": "PT1M"
    },
    "jobSpecification": {
        "priority": 0,
        "constraints": {
            "maxWallClockTime": "PT1H",
            "maxTaskRetryCount": 0
        },
        "jobManagerTask": {
            "id": "report-generator-job-manager-task",
            "commandLine": "cmd /c \"docker run --init --rm --workdir /app -e APPINSIGHTS_INSTRUMENTATIONKEY -e KEY_VAULT_URL -e AZURE_STORAGE_NOTIFICATION_QUEUE '%CONTAINER_REGISTRY_TOKEN%.azurecr.io/batch-report-generator-manager'\"",
            "constraints": {
                "maxWallClockTime": "PT1H",
                "retentionTime": "P3D",
                "maxTaskRetryCount": 0
            },
            "killJobOnCompletion": true,
            "userIdentity": {
                "autoUser": {
                    "scope": "task",
                    "elevationLevel": "admin"
                }
            },
            "runExclusive": false
        },
        "commonEnvironmentSettings": [
            {
                "name": "APPINSIGHTS_INSTRUMENTATIONKEY",
                "value": "%APP_INSIGHTS_TOKEN%"
            },
            {
                "name": "KEY_VAULT_URL",
                "value": "%KEY_VAULT_TOKEN%"
            },
            {
                "name": "AZURE_STORAGE_NOTIFICATION_QUEUE",
                "value": "ondemand-send-notification"
            }
        ],
        "poolInfo": {
            "poolId": "on-demand-scan-request-pool"
        }
    }
}
