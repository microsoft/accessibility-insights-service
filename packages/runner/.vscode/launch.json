{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start debugging runner",
            "type": "node",
            "request": "launch",
            "outFiles": ["${workspaceFolder}/out/**/*.js"],
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/src/index.ts",
            // "preLaunchTask": "tsc",
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "args": [
                "--websiteId=\"706928ee-7cb9-46e5-8a3b-d83864da0727\"",
                "--websiteName=\"MSFT Support\"",
                "--baseUrl=\"https://www.msn.com/\"",
                "--scanUrl=\"https://www.msn.com/\"",
                "--serviceTreeId=\"0b9ad4c5-07d8-4753-baee-e6b39fe7ea48\""
            ]
        },
        {
            "name": "Debug current unit test file",
            "type": "node",
            "request": "launch",
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}/node_modules/jest/bin/jest.js",
                "--runInBand",
                "${fileBasename}",
                "--coverage",
                "false"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "port": 9229
        }
    ]
}
